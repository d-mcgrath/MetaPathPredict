setOldClass(c('tbl_df', 'data.frame'))



setClass(Class = 'MetaPredictInput',
         slots = list(annotations_list = 'list')
)



setClass(Class = 'MetaReconstructInput',
         slots = list(annotations_list = 'list')
)



setClass(Class = 'MetaReconstructResult',
         slots = list(
           summary = 'tbl_df',
           module_reconstructions = 'tbl_df',
           module_extractions = 'tbl_df')
)



setClass(Class = 'MetaPredictResult',
         slots = list(
           summary = 'tbl_df',
           module_reconstructions = 'tbl_df',
           module_predictions = 'tbl_df',
           module_extractions = 'tbl_df')
)
